{
  "name": "church-functions",
  "description": "Cloud Functions for Church App",
  "scripts": {
    "lint": "echo Linting... && echo Lint complete",
    "serve": "firebase emulators:start --only functions,firestore,ui",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "dev": "nodemon --exec \"npm run serve\"",
    "deploy": "firebase deploy --only functions",
    "test-message": "firebase emulators:exec \"node test_send_message.js\"",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0",
    "nodemon": "^3.1.10"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}
